    music: {fileID: 0}
    sound: {fileID: 0}
  - text: Wah kenapa tuh
    audio: {fileID: 8300000, guid: 3e8ab5c0fea76f143b45ae9a1042f4f2, type: 3}
    speaker: {fileID: 11400000, guid: fa0781b12625a7044a716bdf7c9f56bb, type: 2}
